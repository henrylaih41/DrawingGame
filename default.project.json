{
  "name": "CanvasDrawProject",
  "globIgnorePaths": [
    "src/server/tests",
    "src/client/tests",
    "**/*.spec.lua"
  ],
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Sounds": {
        "$className": "Folder"
      },
      "Modules": {
        "$className": "Folder",
        "Canvas": {
          "$className": "Folder",
          "$path": "src/modules/canvas"
        },
        "Services": {
          "$className": "Folder",
          "$path": "src/modules/services"
        },
        "Utils": {
          "$className": "Folder",
          "$path": "src/modules/utils"
        },
        "GameData": {
          "$className": "Folder",
          "$path": "src/modules/gameData"
        }
      },
      "DevPackages": {
        "$className": "Folder",
        "$path": "DevPackages"
      },
      "RemoteEvents": {
        "$className": "Folder"
      }
    },
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$path": "src/client/"
      }
    },
    "ServerScriptService": {
      "$path": "src/server/"
    }
  }
}